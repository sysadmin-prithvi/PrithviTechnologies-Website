---
const { url } = Astro.props;
interface Props {
  url: string;
}
const linkClass =
  "inline-flex h-16 w-10 items-center text-black justify-center gap-x-2 rounded-lg border border-transparent text-sm font-bold  outline-none  hover:text-neutral focus:outline-none focus-visible:ring focus-visible:ring-zinc-500 dark:ring-zinc-200 dark:hover:bg-neutral-50/10";
---

<a class={linkClass} href={url} target="_blank" rel="noopener noreferrer">
  <slot />
</a>
